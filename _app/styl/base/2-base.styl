*
	margin 0
	padding 0
	outline none
body
	font-size: 16px
	font-family: N
	color #000
	background-color #fff
	line-height: 1.56
.wrapper
	overflow hidden
.content
	position relative
	z-index 60
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	&-wrapper
		text-align center
		&-left
			text-align: left
	display inline-block
	margin 0 auto
	text-decoration none
	color: #fff
	width: auto
	font-size: 20px
	font-weight: bold
	padding 20px 115px
	border-radius: 30px
	background-color: #FE7D3B
	&:focus
		text-decoration none
		color #fff
	&:hover
		text-decoration none
		color #fff
	&-main
		background-color: transparent
		border: 2px dotted #000
		color: #000
		&:focus
			color #000
		&:hover
			color #000
	&-price
		width 100%
		padding 14px 50px
		background-color: transparent
		border: 3px solid #FE7D3B
		margin-bottom: 30px
		&&-orange
			border-color: #2C3E54
		@media screen and (max-width 1199px)
			padding 14px 20px
		@media screen and (max-width 991px)
			margin-bottom: 0
.general-title
	text-transform uppercase
	color: #2C3E54
	font-family: G
	font-size: 60px
	// position relative
	text-align center
	line-height: 1.3
	margin-bottom: 60px
	@media screen and (max-width 1199px)
		font-size: 46px
	@media screen and (max-width 991px)
		font-size: 28px
		margin-bottom: 40px
	h3
		display inline-block
	span
		display block
		position relative
		font-family: N
		font-size: 28px
		color #767F8D
		@media screen and (max-width 991px)
			font-size: 24px
	&-left
		span
			top 20px
			left -40px
			text-align: left
			@media screen and (max-width 991px)
				left: -30px
				top 5px
			@media screen and (max-width 610px)
				left -10px
	&-right
		span
			bottom: 10px
			right -40px
			text-align: right
			@media screen and (max-width 991px)
				right: -30px
				bottom: 5px
			@media screen and (max-width 610px)
				right -10px
	&-plan
		color #fff
		text-shadow: 2px 2px 8px rgba(#2C3E54, .8)
	&-simple
		text-align center
		font-size: 28px
		font-weight: bold
		margin-bottom: 60px
		text-transform uppercase
		line-height: 1.2
		@media screen and (max-width 991px)
			font-size: 24px
			margin-bottom: 40px
.general-img
	position absolute
.mob-none
	@media screen and (max-width 991px)
		display none
.d-none
	display none
	@media screen and (max-width 991px)
		display inline-block
// .general-text
// 	margin-bottom: 10px
// 	span
// 		font-weight: bold
// 	&-right
// 		text-align right
.general-list
	margin-left: 30px
	@media screen and (max-width 386px)
		margin-left: 15px
	li
		position relative
		margin-bottom: 10px
		&:last-child
			margin-bottom: 0